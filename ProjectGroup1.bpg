#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = CCCp.exe UnitTests.exe Project1.exe histogram.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

CCCp.exe: CCCp.dpr
  $(DCC)

UnitTests.exe: UnitTests.dpr
  $(DCC)

Project1.exe: other projects\navigator\Project1.dpr
  $(DCC)

histogram.exe: other projects\histogram\histogram.dpr
  $(DCC)


